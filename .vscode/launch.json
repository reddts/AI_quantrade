    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app:app",
                "--reload",
                "--port", "9099" // 指定端口号
            ],
            "env": {
                "ENV_VAR_NAME": "dev" // 可选：设置环境变量
            },
            "justMyCode": true,
            "cwd": "${workspaceFolder}/fastapi-backend",
            "python": "D:\\anaconda3\\envs\\AIquantrade\\python.exe", // 指定Anaconda环境的Python解释器路径
            "jinja": true
        }
    ]
}